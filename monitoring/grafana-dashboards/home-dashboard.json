{
  "title": "Ecommerce Home Dashboard",
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Total Requests Per Second",
      "targets": [
        {
          "expr": "sum(rate(istio_requests_total[1m]))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Active Orders",
      "targets": [
        {
          "expr": "count(http_requests_total{app=\"orders\"})"
        }
      ]
    }
  ]
}
